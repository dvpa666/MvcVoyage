@model  IEnumerable<MvcVoyage.Models.Country>

@{
    ViewBag.Title = "List";
}

<h2>Lista krajów</h2>
<p>
    select from @Model.Count()
    countries:
</p>

<ul>
    @foreach (var country in Model)
    {
        <li>
            @Html.ActionLink(country.Name,"Browse", new { country = country.Name}) 
        </li>
    }
</ul>

